<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.mybatis.mapper.CategoryTypeMapper">
<select id="selectCategoryType" resultType="com.meaningfarm.mall.product.CategoryTypeVO">
	select category_type_no, category_type_name
	from category_type
	order by category_type_no
</select>
</mapper>