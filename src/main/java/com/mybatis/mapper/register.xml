						  , m_type, m_stop, m_point, m_exitstatus, m_exitdate
						  , m_status, m_birth)
		VALUES (#{m_id}, #{auth_id, jdbcType=VARCHAR}, #{m_pw}, #{m_name}, #{m_phone}, #{m_email}
			      , #{m_zipcode}, #{m_address}, #{m_address2}, to_char(sysdate, 'YYYY-MM-DD')
				  , #{m_type, jdbcType=VARCHAR}, 'N', 0, 'N', #{m_exitdate, jdbcType=VARCHAR}
				  , 'N', #{m_birth, jdbcType=VARCHAR})
	</insert>
 -->
